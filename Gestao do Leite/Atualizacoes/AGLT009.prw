Static Function gManut2(dt1,dt2)
Local cAlias     := CriaTrab(Nil,.F.)

incproc("Aguarde! Criando arquivo temporario...")

Criatmp() // Verifica se existe tabela tmp
dbSelectArea("TMP")
DbSetOrder(1)
dbGoTop("TMP")

cQuery := "SELECT * FROM " + RetSqlName("ZLF") + " ZLF "
cQuery += " WHERE ZLF_FILIAL = '" + xFilial("ZLF") + "'"
cQuery += " AND ZLF_DTINI >= '" + DTOS(dt1) + "' AND ZLF_DTFIM <= '" + DTOS(dt2) + "'"
cQuery += " AND D_E_L_E_T_ = ' '"
cQuery += " ORDER BY ZLF_NPAI,ZLF_DESCRI,ZLF_SETOR,ZLF_COD_F,ZLF_LOJA,ZLF_L_LI_R,ZLF_L_TP_L"
If Select("TEMP") <> 0
	TEMP->(dbCloseArea("TEMP"))
Endif
dbUseArea( .T., "TOPCONN", TcGenQry(,,cQuery), "TEMP", .T., .F. )
TEMP->(dbGoTop())
DO WHILE TEMP->(!EOF())
	incproc("Aguarde! Gravando temporario...Fornecedor :"+alltrim(TEMP->ZLF_NREDUZ))
	aArea := GetArea()
	dbSelectArea("TMP")
	DbSetOrder(1)
	Reclock("TMP", .T.)
	Replace TMP->TMP_FILIAL   With XFILIAL("ZLF")
	Replace TMP->TMP_CODZLE   with TEMP->ZLF_CODZLE
	Replace TMP->ZL2_COD      With TEMP->ZLF_COD
	Replace TMP->ZL2_DESCRI   With TEMP->ZLF_DESCRI
	Replace TMP->ZL3_SETOR    With TEMP->ZLF_SETOR
	Replace TMP->ZL3_COD      With TEMP->ZLF_COD_S
	Replace TMP->ZL3_DESCRI   With TEMP->ZLF_DESCR_R
	Replace TMP->A2_NREDUZ    With TEMP->ZLF_NREDUZ
	Replace TMP->A2_COD       With TEMP->ZLF_COD_F
	Replace TMP->A2_LOJA      With TEMP->ZLF_LOJA
	Replace TMP->A2_L_LI_RO   With TEMP->ZLF_L_LI_R
	Replace TMP->A2_L_TP_LR   With TEMP->ZLF_L_TP_L
	Replace TMP->TMP_TOT1     With TEMP->ZLF_TOT1
	Replace TMP->TMP_TOT2     With TEMP->ZLF_TOT2
	Replace TMP->TMP_PAI      With TEMP->ZLF_PAI
	Replace TMP->TMP_NPAI     With TEMP->ZLF_NPAI
	Replace TMP->TMP_DTINI    With DT1
	Replace TMP->TMP_DTFIM    With DT2
	MsUnlock()
	RestArea(aArea)
	TEMP->(DBSKIP())
ENDDO
TEMP->(dbCloseArea("TEMP"))
return

