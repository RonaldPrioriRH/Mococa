Static Function ftela(dt1,dt2,ntipo)
if ntipo = 1
	cQry2 := " SELECT ZLE_COD FROM " + RETSQLNAME("ZLE")
	cQry2 += " WHERE ZLE_FILIAL = '" + xFilial("ZLE") + "'"
	cQry2 += " AND ZLE_DTINI  >= '" + DTOS(dt1) + "'"
	cQry2 += " AND ZLE_DTFIM <= '" + DTOS(dt2) + "'"
	cQry2 += " AND D_E_L_E_T_  <> '*'"
	If Select("TRAB2") <> 0
		TRAB2->(dbCloseArea("TRAB2"))
	Endif
	DbUseArea(.T., "TOPCONN", TCGenQry(,,cQry2), "TRAB2", .F., .T.)
	If !TRAB2->(Eof())
		xmaghelpfis("Atencao","Ja existe movimento lan็ado nesse periodo!","Informe outro periodo!")
		RETURN .F.
	Endif
	TRAB2->(DbCloseArea())
endif
npper := dtoc(dt1)+" a "+ dtoc(dt2)
close(oDlgVen1)
return

