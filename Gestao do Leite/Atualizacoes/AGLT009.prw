Static Function _apdel()
Local _nome
Local _xcha
Local _no := 0

aArea := GetArea()

_nome := LEFT(oEmpTree:GetPrompt(.T.),40)
_xcha := oEmpTree:GetCargo()

dbSelectArea("ZL8")  // TABELA DE EVENTOS
dbSetOrder(1)
dbgotop()
Do while !eof()
	if alltrim(ZL8->ZL8_DESCRI) = ALLTRIM(_nome)
		_no := 1
		exit
	Endif
	dbskip()
Enddo
if _no = 0
	dbSelectArea("ZL3")  // TABELA ROTA/LINHA
	dbSetOrder(1)
	dbgotop()
	Do while !eof()
		if alltrim(ZL3->ZL3_DESCRI) = ALLTRIM(_nome)
			_no := 2
			exit
		Endif
		dbskip()
	Enddo
endif
incproc("Aguarde! Alterando estrutura...")
if _no = 0    // APAGA SOMENTE UM FORNECEDOR
	dbSelectArea("TMP")
	DBGOTOP()
	Do while !tmp->(eof())
		if alltrim(tmp->tmp_chave) = alltrim(_xcha) .and. alltrim(tmp->A2_NREDUZ) = alltrim(_nome)
			Reclock("TMP", .F.)
			dbdelete()
			MsUnlock()
		endif
		tmp->(dbskip())
	Enddo
elseif _no = 1    // APAGA EVENTO TODO
	TMP->(DBGOTOP())
	Do while !tmp->(eof())
		if alltrim(tmp->tmp_npai) = alltrim(_nome)
			Reclock("TMP", .F.)
			dbdelete()
			MsUnlock()
		endif
		tmp->(dbskip())
	Enddo
elseif _no = 2     // APAGAR SETOR/LINHA/ROTA
	_xcha := strzero(val(_xcha)+1,3)
	dbSelectArea("TMP")
	DBGOTOP()
	Do while !tmp->(eof())
		if alltrim(tmp->tmp_chave) = alltrim(_xcha) .and. alltrim(tmp->ZL3_DESCRI) = alltrim(_nome)
			Reclock("TMP", .F.)
			dbdelete()
			MsUnlock()
		endif
		tmp->(dbskip())
	Enddo
endif

RestArea(aArea)

oEmpTree:DelItem()
oEmpTree:Refresh()
oEmpTree:SetFocus()
return .t.
