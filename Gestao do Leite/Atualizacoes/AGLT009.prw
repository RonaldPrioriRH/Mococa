Static Function GravReg(dt1,dt2,ntipo)

Local _nxcod

dbSelectArea("TMP")
DbSetOrder(1)
dbgotop()
if ntipo = 1
	_nxcod := GetSxeNum("ZLE","ZLE_COD")
	ConfirmSX8()
elseif ntipo = 2  
	_nxcod := TMP->TMP_CODZLE
	cQuery3 := "DELETE FROM " + RetSqlName("ZLE") + " "
	cQuery3 += "WHERE ZLE_FILIAL = '" + xFilial("ZLE") 	+ "' AND "
	cQuery3 += "ZLE_COD = '"  + _nxcod + "' AND "
	cQuery3 += "D_E_L_E_T_ = ' '"
	TcSqlExec(cQuery3)
	
	cQuery := "DELETE FROM " + RetSqlName("ZLF") + " "
	cQuery += "WHERE ZLF_FILIAL = '" + xFilial("ZLF") + "' AND "
	cQuery += "ZLF_CODZLE = '"  + _nxcod + "' AND "
	cQuery += "D_E_L_E_T_ = ' '"
	TcSqlExec(cQuery)
	
endif

dbSelectArea("ZLE")     /// grava cabecario do mix
Reclock("ZLE", .T.)
Replace ZLE_FILIAL with xfilial("ZLE")
Replace ZLE_COD    With _nxcod
Replace ZLE_DTINI  With TMP->TMP_DTINI
Replace ZLE_DTFIM  With TMP->TMP_DTFIM
MsUnlock()

dbSelectArea("TMP")
DbSetOrder(1)
dbgotop()
DO WHILE TMP->(!EOF())
	aArea := GetArea()
	
	dbSelectArea("ZLF")     // grava itens do mix
	Reclock("ZLF", .T.)
	Replace ZLF_FILIAL   With XFILIAL("ZLF")
	Replace ZLF_CODZLE   With _nxcod
	Replace ZLF_COD      With TMP->ZL2_COD
	Replace ZLF_DESCRI   With TMP->ZL2_DESCRI
	Replace ZLF_SETOR    With TMP->ZL3_SETOR
	Replace ZLF_COD_S    With TMP->ZL3_COD
	Replace ZLF_DESCR_R  With TMP->ZL3_DESCRI
	Replace ZLF_NREDUZ   With TMP->A2_NREDUZ
	Replace ZLF_COD_F    With TMP->A2_COD
	Replace ZLF_LOJA     With TMP->A2_LOJA
	Replace ZLF_L_LI_R   With TMP->A2_L_LI_RO
	Replace ZLF_L_TP_L   With TMP->A2_L_TP_LR
	Replace ZLF_TOT1     With TMP->TMP_TOT1
	Replace ZLF_TOT2     With TMP->TMP_TOT2
	Replace ZLF_PAI      With TMP->TMP_PAI
	Replace ZLF_NPAI     With TMP->TMP_NPAI
	Replace ZLF_DTINI    With TMP->TMP_DTINI
	Replace ZLF_DTFIM    With TMP->TMP_DTFIM
	MsUnlock()
	
	RestArea(aArea)
	TMP->(DBSKIP())
ENDDO
_fecha()
return


