Static Function gInclu(nvend,ncod,dt1,dt2,np)
Local cAlias     := CriaTrab(Nil,.F.)
Local _grava     := 1   // valida se teve movimento para o produto
IF np ="1"              // NAO RETIRAR ESSA LINHA
	Close(oDlgVen1)
endif
Criatmp()               // verifica se existe tabela temporaria
dbSelectArea("TMP")
DbSetOrder(1)
dbGoTop("TMP")
cQuery := "SELECT ZL2_DESCRI, ZL3_SETOR,ZL3_COD,ZL3_DESCRI,ZL2_COD,A2_NREDUZ,A2_COD,A2_L_TPFOR,A2_CGC,A2_LOJA,A2_NOME,A2_L_LI_RO,A2_L_TP_LR FROM " + RetSqlName("ZL2") + " ZL2, "
cQuery +=  RetSqlName("ZL3") + " ZL3, "
cQuery +=  RetSqlName("SA2") + " SA2 "
cQuery += " WHERE ZL3_FILIAL = '" + xFilial("ZL3") + "'"
cQuery += " AND ZL2_FILIAL = '" + xFilial("ZL2") + "'"
cQuery += " AND A2_FILIAL = '" + xFilial("SA2") + "'"
cQuery += " AND ZL3_SETOR = ZL2_COD"
cQuery += " AND A2_L_LI_RO = ZL3_COD"
cQuery += " AND (ZL3.D_E_L_E_T_ = ' ')"
cQuery += " AND (ZL2.D_E_L_E_T_ = ' ')"
cQuery += " AND (SA2.D_E_L_E_T_ = ' ')"
cQuery += " ORDER BY ZL2_DESCRI,ZL3_SETOR,A2_COD,A2_LOJA,A2_L_LI_RO,A2_L_TP_LR"
If Select("TEMP") <> 0
	TEMP->(dbCloseArea("TEMP"))
Endif
dbUseArea( .T., "TOPCONN", TcGenQry(,,cQuery), "TEMP", .T., .F. )
TEMP->(dbGoTop())
DO WHILE TEMP->(!EOF())
	aArea := GetArea()
	incproc("Aguarde! Gravando temporario...Fornecedor :"+alltrim(TEMP->A2_NREDUZ))
	If !upper(TEMP->A2_L_TPFOR) $ "P/G"   // verifica se o fornecedor e produtor
	   TEMP->(DBSKIP())
	   loop
	Endif
	_TBOM := 0   // total do leite bom
	_TACI := 0   // total do leite acido
	cQry1 := " SELECT SUM(ZLD_TOTBOM) TBOM, SUM(ZLD_TOTACI) TACI FROM " + RETSQLNAME("ZLD")
	cQry1 += " WHERE ZLD_FILIAL = '" + xFilial("ZLD") + "'"
	cQry1 += " AND ZLD_DTCOLE >=  '" + DTOS(dt1) + "' AND ZLD_DTCOLE <=  '" + DTOS(dt2) + "'"
	cQry1 += " AND ZLD_RETIRO = '" + TEMP->A2_COD + "'"
	cQry1 += " AND ZLD_RETILJ = '" + TEMP->A2_LOJA + "'"
	cQry1 += " AND D_E_L_E_T_  <> '*'"
	If Select("TRAB1") <> 0
		TRAB1->(dbCloseArea("TRAB1"))
	Endif
	DbUseArea(.T., "TOPCONN", TCGenQry(,,cQry1), "TRAB1", .F., .T.)
	If !TRAB1->(Eof())
		_TBOM := TRAB1->TBOM
		_TACI := TRAB1->TACI
	Endif
	_grava := 1  // valida se teve movimento para o produto
	if _TBOM > 0
		_grava := 0
	endif
	if _TACI > 0
		_grava := 0
	endif
    if _grava = 0 // se teve movimento grava
		TRAB1->(DbCloseArea())
		dbSelectArea("TMP")
		DbSetOrder(1)
		Reclock("TMP", .T.)
		Replace TMP_FILIAL  With xfilial("ZL3")
		Replace ZL2_COD     With TEMP->ZL2_COD
		Replace ZL2_DESCRI  With TEMP->ZL2_DESCRI
		Replace ZL3_SETOR   With TEMP->ZL3_SETOR
		Replace ZL3_COD     With TEMP->ZL3_COD
		Replace ZL3_DESCRI  With TEMP->ZL3_DESCRI
		Replace A2_NREDUZ   With TEMP->A2_NREDUZ
		Replace A2_COD      With TEMP->A2_COD
		Replace A2_CGC      With TEMP->A2_CGC
		Replace A2_LOJA     With TEMP->A2_LOJA
		Replace A2_NOME	 With TEMP->A2_NOME
		Replace A2_L_LI_RO  With TEMP->A2_L_LI_RO
		Replace A2_L_TP_LR  With TEMP->A2_L_TP_LR
		Replace TMP_TOT1    With _TBOM
		Replace TMP_TOT2    With _TACI
		Replace TMP_PAI     With ncod
		Replace TMP_NPAI    With nvend
		Replace TMP_DTINI   With dt1
		Replace TMP_DTFIM   With dt2
		MsUnlock()
	Endif
	RestArea(aArea)
	TEMP->(DBSKIP())
ENDDO
TEMP->(dbCloseArea("TEMP"))

return



