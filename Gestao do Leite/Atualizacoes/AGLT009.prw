Private oFonTm1
DEFINE FONT oFonTm1 NAME "Courier New" BOLD
Private oFont9N	 := TFont():New("Courier New",18,9 ,.T.,.F.,10,.T.,10,.T.,.F.)
Private oFont8N	 := TFont():New("Courier New",18,9  ,.T.,.F.,8,.T.,8,.T.,.F.)

