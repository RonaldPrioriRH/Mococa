Private oVlrPed, oDesPed, oLiqPed, oVlrPrc, oSubPrc, oVlrTro, oVlrNCC, oVlrDes, oDesTot, oTipVen,oHReal,oVlrPr,oNov,cArqSD3,cAux,cAux3,npper,oLbDet,odtm,oLbDet,oTree,ntelax
Private cLbl001  := "MIX"
Private dt       := dDataBase
Private dt1      := ddatabase
Private dt2      := ddatabase+15
Private aSize    := MsAdvSize()
Private aObjects := {}
Private aInfo    := {}
Private aArrFon  := {}
Private aDados   := {}
Private aDados2  := {}
Private aGer	 := {}
Private aDet	 := {}
Private aGer	 := {}
Private nOpc1    := 1
Private nBcoHdl  := 0
Private ngeral   := 0
Private cFolderA := 'FOLDER5'
Private cFolderB := 'FOLDER6'
Private ntitulo  := ""
Private cFile    := ""
Private cType    := ""
Private nvend    := ""

If Select("TMP")<>0    // fecha tabela temporaria
	dbSelectArea("TMP")
	dbCloseArea ("TMP")
EndIf

aAdd(aArrFon, oFonTm1)
aAdd(aArrFon, oFont9N)
aAdd(aArrFon, oFont8N)

AAdd( aObjects, { 100, 800 , .T., .T. } )
AAdd( aObjects, { 100,  8 , .T., .T. } )
AAdd( aObjects, { 10,  10 , .T., .T. } )
AAdd( aObjects, { 10,  8 , .T., .T. } )
AAdd( aObjects, { 10,  10 , .T., .T. } )

AAdd( aObjects, { 000,  10 , .T., .T. } )

aInfo   := { aSize[ 1 ], aSize[ 2 ], aSize[ 3 ], aSize[ 4 ], 3, 3 }
aPosObj := MsObjSize( aInfo, aObjects , .T. )

if ntipo = 1 // SE INCLUSAO
	ntitulo := " Inclusใo do Mix"
	GLTINC(ntipo)    // chama tela para informar o periodo da tela do mix
	IF empty(npper)  // SE PERIODO ESTIVER VAZIO RETORNA - NAO TIRAR ESSA VALIDACAO
		return
	endif
	dbSelectArea("ZL8")  // TABELA DE EVENTOS
	dbSetOrder(1)
	dbgotop()
	While ZL8->(!eof())
		aArea4 := GetArea()
		nvend  :=ZL8->ZL8_DESCRI
		geratmp(nvend,ZL8->ZL8_COD,dt1,dt2,"0") // alimenta tabela temporaria para inclusao
		RestArea(aArea4)
		ZL8->(dbSkip())
	Enddo
elseif ntipo =2  // SE MANUTENCAO
	ntituo := " Manuten็ใo do Mix"
	DT1    := ZLE->ZLE_DTINI  // DATA INICIAL PARA MANUTENCAO
	DT2    := ZLE->ZLE_DTFIM  // DATA FINAL PARA MANUTENCAO
	npper  := dtoc(dt1)+" a "+ dtoc(dt2)
	ncod   := ""
	gManut(dt1,dt2) // carrega manutencao no tree
endif
DEFINE MSDIALOG oDlgVen TITLE ntitulo FROM aSize[7],0 to aSize[6],aSize[5]  PIXEL
oPanCon := TPanel()	:New(00,000, "", oDlgVen, Nil, .T., .F., Nil, Nil, 640,1300, .T., .F. )

@ 005,002 Say "Periodo :" Pixel Of oPanCon Font aArrFon[1] Color CLR_BLACK
@ 005,030 SAY odtm var npper Pixel Of oPanCon Font aArrFon[1] Color CLR_BLUE

DEFINE DBTREE oEmpTree FROM aPosObj[1,1] ,aPosObj[1,2] TO aPosObj[1,3] ,aPosObj[1,4] OF oPanCon CARGO

oEmpTree:= DbTree():New(aPosObj[1,1] ,aPosObj[1,2],aPosObj[1,3] ,aPosObj[1,4], oDlgVen,,,.T.)
geratela() // gera a arvore

@ 270, 010 say "F5 - Inclui Evento"  Pixel Of oPanCon Font aArrFon[1] Color CLR_BLUE
@ 280, 010 say "F6 - Exclui"  Pixel Of oPanCon Font aArrFon[1] Color CLR_RED
@ 290, 010 say "F7 - Inclui Valores"  Pixel Of oPanCon Font aArrFon[1] Color CLR_BLUE
@ 295, 595 BmpButton Type 01 Action GravReg(dt1,dt2,ntipo) // GRAVA INCLUSAO/ALTERACAO DA TELA DO MIX
@ 295, 550 BmpButton Type 02 Action _fecha() // FECHA A TELA DO MIX E NAO GRAVA NADA

SET KEY VK_F5 TO //GLTINC2()   // inclui eventos
SET KEY VK_F6 TO GLTEXC()    // exclui eventos
SET KEY VK_F7 TO digprc()    // grava valor no item da arvore
SET KEY VK_F8 TO _MSBROWSE() // visualiza arq. temporario
SET KEY VK_F11 TO 

ACTIVATE DIALOG oDlgVen CENTER

Return .t.

