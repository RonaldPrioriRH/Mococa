Static function geratela()

Local cAlias     := CriaTrab(Nil,.F.)
Private aSetor   := {}
Private aSetor1  := {}
Private aRotLin  := {}
Private aRotLin1 := {}
Private	_TBOM    := 0
Private	_TACI    := 0
Private ntp      := ""     // Chave de busca no tree
Private nant     := ""     // Chave de busca no tree
Private ntp      := ""
Private _IPAI    := " "
Private nmut     := 1
Private cAux1,_proc,cArl,cArl2,cAux2
Private ntp,nant,rotlin,nant1

Criatmp()                  // Verifica se existe o arquivo temporario

oEmpTree:SetFont(oFonTm1)  // Inicia gravacao no objeto tree
oEmpTree:Reset()           // resta o tree
oEmpTree:BeginUpdate()     // prepara para receber gravacao
oEmpTree:AddItem("Gestao do Leite"+SPACE(58)+"Volume Bom"+space(25)+"Volume Acido"+space(25)+"Valor",'001','BPMSEDT2' ,,,,1)

dbSelectArea("TMP")
DbSetOrder(1)
dbGoTop("TMP")
While TMP->(!EOF())
	
	IF ALLTRIM(TMP->TMP_PAI) <> ALLTRIM(_IPAI)
		_IPAI   := TMP->TMP_PAI
		ntp     := TMP->TMP_NPAI
		_proc   := ALLTRIM(TMP->TMP_NPAI)
		Gravchv(ntp) // grava chave de referencia
		oEmpTree:TreeSeek("001",.T.)
		oEmpTree:AddItem(TMP->TMP_NPAI,ntp,"BPMSEDT2","BPMSEDT1",,,)
		nant    := TMP->ZL2_DESCRI
	else
		nant    := ALLTRIM(TMP->ZL2_DESCRI)				
	endif 
	if ALLTRIM(TMP->ZL2_DESCRI) = ALLTRIM(nant) .and. ALLTRIM(TMP->TMP_PAI) = ALLTRIM(_IPAI)
		nant    := ALLTRIM(TMP->ZL2_DESCRI)
		nmut     = nmut+1
		nant1   := strzero(nmut,3)
		Gravchv(nant1) // grava chave de referencia
		if oEmpTree:TreeSeek(ntp)			 
			oEmpTree:AddItem(TMP->ZL2_DESCRI,nant1,"BPMSEDT2","BPMSEDT1",,,)
			oEmpTree:TreeSeek(nant1)
		endif
		
		if alltrim(TMP->ZL2_DESCRI) = alltrim(nant)
			While alltrim(TMP->ZL2_DESCRI) = alltrim(nant) .and. ALLTRIM(TMP->TMP_PAI) = ALLTRIM(_IPAI)
				nlin   := alltrim(TMP->ZL3_DESCRI)
				nmut    = nmut+ 1
				rotlin := STRzero(nmut,3)
				
				IF nlin = ALLTRIM(TMP->ZL3_DESCRI) .and. ALLTRIM(TMP->TMP_PAI) = ALLTRIM(_IPAI)
					
					If oEmpTree:TreeSeek(nant1)
						Gravchv(rotlin) // grava chave de referencia
						oEmpTree:AddItem(TMP->ZL3_DESCRI,rotlin,"BPMSEDT2","BPMSEDT1",,,)
						oEmpTree:TreeSeek(rotlin)
					Endif
					nmut = nmut+1
					rotlin := STRzero(nmut,3)
					
					IF alltrim(TMP->ZL3_DESCRI) = alltrim(nlin) .and. ALLTRIM(TMP->TMP_PAI) = ALLTRIM(_IPAI)
						While alltrim(TMP->ZL3_DESCRI) = alltrim(nlin) .and. ALLTRIM(TMP->TMP_PAI) = ALLTRIM(_IPAI)
							
							_TBOM := tmp->tmp_tot1
							_TACI := tmp->tmp_tot2
							
							Gravchv(rotlin) // grava chave de referencia
							_ctext:=TMP->A2_NREDUZ+SPACE(40)+Transform( _TBOM,"@E 999,999,999")+SPACE(26)+Transform( _TACI,"@E 999,999,999")+SPACE(26)+Transform(TMP_VALOR,"@E 999.9999")
							oEmpTree:AddItem(_ctext,rotlin,"BPMSEDT2","BPMSEDT1",,,)
							
							_TBOM := 0
							_TACI := 0
							
							TMP->(dbSkip())
							
							IF ALLTRIM(TMP->ZL2_DESCRI) <> alltrim(nant)
								exit
							Endif
						Enddo
					Else
						TMP->(dbSkip())				
						nlin := ALLTRIM(TMP->ZL3_DESCRI)
					Endif
				Else
					TMP->(dbSkip())
					nlin := ALLTRIM(TMP->ZL3_DESCRI)
				Endif
			Enddo
		Else
			TMP->(dbSkip())
			if  ALLTRIM(TMP->TMP_PAI) = ALLTRIM(_IPAI)
				nant    := ALLTRIM(TMP->ZL2_DESCRI)
			ENDIF
			LOOP
		Endif
	Endif
Enddo
oEmpTree:TreeSeek("001",.T.)
oEmpTree:EndUpdate()
return

