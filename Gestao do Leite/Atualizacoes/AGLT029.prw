getaCols()

ACTIVATE MSDIALOG oDlg ON INIT EnchoiceBar(oDlg,{|| if(nOpc!=5,lConfirmou := tudoOK(),lConfirmou := .T.) ,if(lConfirmou,oDlg:End(),)},{||oDlg:End()},,aButtons)



//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³ Grava dados da ZLD                                   ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
if lConfirmou
	dbSelectArea("ZLD")

	BEGIN TRANSACTION
	if nOpc == 3
		//cCodRec := getSxeNum("ZLD","ZLD_CODREC")
		//cCodRec := getNumRec()
		insZLD(aCols)
	elseif nOpc == 4
		//Verifica se houve alteracao
		if alterou(aColsAux,aCols)
			//Apaga Itens Anteriores
			delZLD(aColsAux)
			//Grava Novos Itens
			insZLD(aCols)
		endif
	elseif nOpc == 5
		//Apaga Itens
		delZLD(aColsAux)
	endif
	
	ConfirmSx8()
	END TRANSACTION
else
	
endif
Return

