Static Function getDFret(a_cod,a_lj)
Local cArea := getArea()
Local bret:= .f.      

if empty(a_lj)
     return
endif

cQuery := "SELECT *  FROM "
cQuery += RetSqlName("ZL3") + " WHERE D_E_L_E_T_ = ' ' "
cQuery += "  AND ZL3_FILIAL = '" + xFilial("ZL3") + "'"
cQuery += "  AND ZL3_FRETIS = '" + a_cod + "'"
cQuery += "  AND ZL3_FRETLJ = '" + a_lj + "'"

dbUseArea(.T.,"TOPCONN",TCGenQry(,,ALLTRIM(Upper(cQuery))),'TRB',.F.,.T.)

If TRB->(!Eof())
	cLinRot := TRB->ZL3_COD
	cSetor	:= TRB->ZL3_SETOR
	cVeicul := TRB->ZL3_VEICUL
	bret := .t.
EndIf
TRB->(dbclosearea())

if !bRet
	if MsgYesNo("Atencao, esse Transportador não está vinculado a nenhuma linha! Continuar assim mesmo?")
		bRet:=.t.
	endif
endif

restArea(cArea)
return bRet

