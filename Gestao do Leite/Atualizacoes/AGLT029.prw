static function vldSet(cSetor)
local lRet := .T.
local aArea := getArea()

if !empty(cSetor)
	dbSelectArea("ZL2")
	dbSetOrder(1)
	lRet :=  dbSeek(xFilial("ZL2")+cSetor)
	
	if lRet
		cDescSet := ZL2->ZL2_DESCRI
	endif
endif

restArea(aArea)

return lRet

