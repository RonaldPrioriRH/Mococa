static function linOK()
local lRet := .T.
local aAux := aCols[n] //Pega Linha Atual
local aAux1 := {}
local nIndex := 0
local cBlq := ""

//Atualiza Variaveis de Totais
nLeiteBom := nVolAnt+getTotCol()
nTotCodRec:= u_getTotCol()
nLtDif := nTotBom-nLeiteBom

oLteBom:Refresh()
nDif:Refresh()
oTotCodRec:Refresh()
oDlg:Refresh()

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³ Verifica se a linha foi excluida                     ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
if aTail(aCols[n])
	return .T.
endif

if len(aCols) > 1
	for i := 1 to len(aCols)
		if i != n .and. !aTail(aCols[i])
			aadd(aAux1,aCols[i])
		endif
	next i
	
	nIndex := ascan(aAux1,{|x| x[nPosRetiro] == aAux[nPosRetiro] .and. x[nPosLoja] == aAux[nPosLoja] })
	lRet := nIndex == 0
	if !lRet .and. !lMensal // Mensal
		xmaghelpfis("Retiro","Produtor já incluído neste recebimento. Nao pode haver dois produtores na mesma coleta!","Lance todos os volumes num só Produtor!")
		return lRet
	endif
endif

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³ Verifica se o retiro esta dentro da linha informada  ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
if !aTail(aCols[n])  
	cLinhaRota := Posicione("SA2",1,xFilial("SA2")+aAux[nPosRetiro]+aAux[nPosLoja],"A2_L_LI_RO")
	lRet := cLinhaRota == cLinRot
	if !lRet
		xmaghelpfis("Atencao","Produtor informado não pertence a linha/rota informada","Selecione um Produtor que seja da linha informada.")
		return lRet
	endif
	cBlq := Posicione("SA2",1,xFilial("SA2")+aAux[nPosRetiro]+aAux[nPosLoja],"A2_MSBLQL")
	lRet := cBlq == "2"//2=Liberado; 1=Bloqueado
	if !lRet
		xmaghelpfis("Atencao","Produtor informado não está ativo no SIGSIF.","Ative o Produtor no cadastro de Fornecedores ou selecione outro produtor!")
		return lRet
	endif
endif

// Mensal
if lMensal
	for y:=1 to len(acols)
		aCols[y,nPosRetiro]	:=	aCols[1,nPosRetiro]
		aCols[y,nPosLoja]	:=	aCols[1,nPosLoja]
		aCols[y,nPosNomRet]	:=	aCols[1,nPosNomRet]
	next y
endif

if aAux[nPosQtdbom] <= 0
	xmaghelpfis("Atencao","Quantidade de leite deve ser informada!","Informe a quantidade de leite do produtor!")
	return .f.
endif

return lRet


