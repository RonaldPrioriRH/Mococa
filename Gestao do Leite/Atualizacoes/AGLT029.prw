static function vldFretist(cFretist,cLjFret)
local lRet := .T.
local cLoja := if(!empty(cLjFret),cLjFret,"0001") //Seta var loja com loja padrao caso a mesma esteja vazia
local aArea := getArea()

if !empty(cFretist)
	dbSelectArea("SA2")
	dbSetOrder(1)
	lRet := dbSeek(xFilial("SA2")+cFretist+cLoja)
	
	
	if lRet
		cDescFret := SA2->A2_NOME
	endif
endif

restArea(aArea)
return lRet

