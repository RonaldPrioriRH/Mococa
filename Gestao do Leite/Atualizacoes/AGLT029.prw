static function vldLinRot(cLinRot)
local lRet := .T.
local aArea := getArea()

if !empty(ALLTRIM(cLinRot))
	dbSelectArea("ZL3")
	dbSetOrder(1)
	lRet := dbSeek(xFilial("ZL3")+cLinRot)
	
	if lRet
		cTipLR  := ZL3->ZL3_TIPO
		lPlataforma := IIF(ZL3->ZL3_PLATAF=="S",.t.,.f.)
		if !lPlataforma
			cVeicul := ZL3->ZL3_VEICUL
			vldVeicul(cVeicul)
			cMotor := POSICIONE("ZL4",1,XFILIAL("ZL4")+ZL3->ZL3_VEICUL,"ZL4_MOTORI")
			vldMotor(cMotor)
		endif
		cDescLin := ZL3->ZL3_DESCRI
		nTotKm:=ZL3->ZL3_KM
	endif
	
	if lRet
		lRet := ZL3->ZL3_SETOR == cSetor
		if !lRet
			xmaghelpfis("Atencao","O Setor selecionado nao pertence a essa Linha","Selecione uma linha que faca parte do setor informado!")
			oLinRota:SetFocus()
		endif
	endif
	
	//Mensal
	if lRet .and. !lPlataforma .and. lMensal
		xmaghelpfis("Atencao","Somente plataformas podem ser lancadas mensalmente!","Selecione uma plataforma ou entre no modo de Recepcao de Leite Diaria.")
		lRet:=.f.
	endif
	
endif


if lRet .AND. LEFT(XFILIAL("ZLD"),1)=="1"
	if lMensal
			getDatas()
	else
			getProdutores()
	endif
endif

restArea(aArea)
