static function getaCols()
local aArea := getArea()

if type("aCols") != "U"
	aColsAux := aClone(aCols)
endif

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³ Atualiza campo nomeRetiro na getdados                ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
for i := 1 to len(aCols)
	if ((!empty(aCols[i,nPosRetiro])) .and. (!empty(aCols[i,nPosLoja])) )
		aCols[i,nPosNomRet] := Posicione("SA2",1,xFilial("SA2")+aCols[i,nPosRetiro]+aCols[i,nPosLoja],"A2_NOME")
	endif
next i

restArea(aArea)
return

