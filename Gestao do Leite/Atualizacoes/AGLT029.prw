Static Function getDatas()
Local aArea := GetArea()
Local nRet:=0
Local dAux:=dDtColeta
Local nDias := 0

while dAux <= lastday(dDtColeta)
    nDias++
	if nDias == 1
		aCols[nDias,nPosDataCol] := dAux
	else
		Aadd(aCols,{dAux,"","",0,"",,,.F.})
    endif
    dAux++
enddo

oGet:ForceRefresh()
oGet:Refresh()
	    
return nRet
