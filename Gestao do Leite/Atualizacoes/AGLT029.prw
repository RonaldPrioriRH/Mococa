DEFINE MSDIALOG oDlg TITLE cTitulo OF oMainWnd PIXEL FROM aSize[7],0 TO aSize[6],aSize[5]
@ 1.3,0.3 TO 2.3,43.0
@ 5.9,0.3 TO 7.0,43.0

@ 1.6,00.7 		SAY "Data Coleta"
@ 1.5,04.7  	MSGET dDtColeta  Valid CheckSX3("ZLD_DTLANC") .and.  VldUser('ZLD_DTLANC') WHEN (nOpc==3)
@ 1.6,12.2  	SAY "Data Lanc."
@ 1.5,16.0 		MSGET dData Valid CheckSX3("ZLD_DTLANC") .and.  VldUser('ZLD_DTLANC') WHEN .F.

@ 2.6,00.7 	SAY "Transport."
@ 2.5,04.7  MSGET cFretist Valid iif(empty(cFretist),.t.,CheckSX3("ZLD_FRETIS") .and.  VldUser('ZLD_FRETIS') .and. vldFretist(cFretist,cLjFret) .and. getDFret(cFretist,cLjFret)) WHEN (nOpc==3)  F3 "SA2_L3"
@ 2.5,09.5  MSGET cLjFret  Valid iif(empty(cLjFret) ,.t.,CheckSX3("ZLD_LJFRET") .and.  VldUser('ZLD_LJFRET') .and. vldFretist(cFretist,cLjFret) .and. getDFret(cFretist,cLjFret)) WHEN (nOpc==3)
@ 2.5,12.2  MSGET cDescFret  WHEN .F.

@ 3.6,00.5 	SAY "Setor"
@ 3.5,04.7  MSGET oSetor var cSetor Valid iif(empty(cSetor),.t.,CheckSX3("ZLD_SETOR") .and.  VldUser('ZLD_SETOR') .and.  vldSet(cSetor)) .and. getNwTicket() WHEN (nOpc==3) F3 "ZL2_01"
@ 3.5,09.5  MSGET cDescSet  WHEN .F.

@ 4.6,00.7  SAY "Linha/Rota"
@ 4.5,04.7 	MSGET oLinRota var cLinRot  Valid iif(empty(cLinRot),.t.,CheckSX3("ZLD_LINROT") .and.  VldUser('ZLD_LINROT') .and. vldLinRot(cLinRot)) WHEN (nOpc==3) F3 "ZL3_02"
@ 4.5,09.5  MSGET cDescLin  WHEN .F.
@ 4.6,23.1  SAY "Total KM"
@ 4.5,26.1  MSGET nTotKm Picture "@E 9,999.99" Valid iif(empty(ntotkm),.t.,CheckSX3("ZLD_KM") .and.  VldUser('ZLD_KM') ) WHEN (nOpc==3)

@ 5.6,00.7 	SAY "Motorista"
@ 5.5,04.7  MSGET cMotor Valid iif(empty(cMotor),.t.,CheckSX3("ZLD_MOTOR") .and.  VldUser('ZLD_MOTOR') .and. vldMotor(cMotor) ) WHEN (nOpc==3) F3 "ZL0_02"
@ 5.5,09.5  MSGET cDescMot  WHEN .F.

if lFlat
	@ 5.6,23.1  	SAY "Veiculo"
	@ 5.5,26.1  	MSGET cVeicul Picture PesqPict("ZLD","ZLD_VEICUL") Valid iif(empty(cVeicul),.t., CheckSX3("ZLD_VEICUL") .and.  VldUser('ZLD_VEICUL') .and. vldVeicul(cVeicul)) WHEN (nOpc==3)  F3 "ZL4_01"
	@ 5.5,30.1 		MSGET cPlacaVeic  WHEN .F.
else
	@ 5.6,26.7  	SAY "Veiculo"
	@ 5.5,29.2  	MSGET cVeicul Picture PesqPict("ZLD","ZLD_VEICUL") Valid iif(empty(cVeicul),.t.,CheckSX3("ZLD_VEICUL") .and.  VldUser('ZLD_VEICUL') .and. vldVeicul(cVeicul)) WHEN (nOpc==3)  F3 "ZL4_01"
	@ 5.5,34.2 		MSGET cPlacaVeic  WHEN .F.
endif

nLeiteBom  := nVolAnt+getTotCol()
nTotCodRec := getTotCol()
nLtDif := nTotBom-nLeiteBom

DEFINE FONT oFont1 NAME "Tahoma" BOLD
@ 6.6,00.7 SAY "Ticket" FONT oFont1
@ 6.5,04.7 MSGET cTicket Picture PesqPict("ZLD","ZLD_TICKET") Valid CheckSX3("ZLD_TICKET") .and.  VldUser('ZLD_TICKET') .and. !isExist(cTicket,"ZLD","ZLD_TICKET") WHEN (nOpc==3) FONT oFont1
