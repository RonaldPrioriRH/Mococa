	aYesFields	:= {"ZLD_RETIRO","ZLD_RETILJ","ZLD_PRODUT","ZLD_LOCAL","ZLD_QTDBOM","ZLD_DCRRET","ZLD_TQ_LT","ZLD_DTCOLE"}
else
	aNoFields	:= {"ZLD_TICKET","ZLD_DTLANC","ZLD_SETOR","ZLD_LINROT","ZLD_TP_RL","ZLD_FRETIS","ZLD_LJFRET","ZLD_VEICUL","ZLD_MOTOR","ZLD_KM","ZLD_CODREC","ZLD_PRODUT","ZLD_LOCAL","ZLD_TOTREC","ZLD_STATUS","ZLD_USER","ZLD_TOTBOM","ZLD_TOTACI","ZLD_TOTAL","ZLD_DTCOLE","ZLD_QTDACI","ZLD_VFRELT","ZLD_VFREPR","ZLD_ALI_WT","ZLD_REC_WT"}
	aYesFields	:= {"ZLD_RETIRO","ZLD_RETILJ","ZLD_PRODUT","ZLD_LOCAL","ZLD_QTDBOM","ZLD_DCRRET","ZLD_TQ_LT"}
endif

aButtons  := If(Type("aButtons") == "U", {}, aButtons)


