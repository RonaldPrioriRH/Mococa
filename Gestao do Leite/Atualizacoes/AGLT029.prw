		AAdd( _aAutoSD3,{"D3_COD"		, SD3->D3_COD	, nil } )	// Unidade de Medida			: Obrigatorio
		AAdd( _aAutoSD3,{"D3_DOC"	    , SD3->D3_DOC	, nil } )	// Quantidade de Leite
