If inclui .OR. altera
	bOk := gerMI(cTicket,3)	
endif

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³ Se gravou Mi entrao grava recepcao                   ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
if bOk
	cCodRec := getnumrec()

	//  -----------------------------------------------------
    //  Se nao houver nenhum item grava um registro em branco
	//  Apenas para gerar estoque
	//  -----------------------------------------------------
	if len(aRegs) == 1 .AND. empty(aRegs[1,nPosRetiro]) .AND. empty(aRegs[1,nPosQtdBom])
			recLock("ZLD",.T.)
			ZLD->ZLD_FILIAL		:= 	xFilial("ZLD")
			ZLD->ZLD_CODREC		:= 	cCodRec
			ZLD->ZLD_TICKET		:=  cTicket
			ZLD->ZLD_DTLANC		:=  dData
			ZLD->ZLD_DTCOLE		:= 	dDtColeta
			ZLD->ZLD_SETOR 		:= 	cSetor
			ZLD->ZLD_LINROT		:= 	cLinRot
			ZLD->ZLD_TP_RL		:= 	cTipLR
			ZLD->ZLD_FRETIS		:=	cFretist
			ZLD->ZLD_LJFRET		:= 	cLjFret
			ZLD->ZLD_VEICUL		:= 	cVeicul
			ZLD->ZLD_MOTOR		:= 	cMotor
			ZLD->ZLD_KM			:= 	nTotKm
			ZLD->ZLD_STATUS		:= 	" "
			ZLD->ZLD_USER 		:= cUser
			ZLD->ZLD_TOTBOM		:= nTotBom
			ZLD->(MsUnlock())
			ZLD->(dbCommit())
	endif
	for i := 1 to len(aRegs)
		if !aTail(aRegs[i])	 //Verifica se item esta deletado
			if (!empty(aRegs[i,nPosRetiro]) .and. !empty(aRegs[i,nPosLoja]))
				nProp  := (aRegs[i,nPosQtdBom]) / (nTotBom) //Acha proporcao pelo volume do produtor e volume total
				recLock("ZLD",.T.)
				ZLD->ZLD_FILIAL		:= 	xFilial("ZLD")
				ZLD->ZLD_CODREC		:= 	cCodRec
				ZLD->ZLD_TICKET		:=  cTicket
				ZLD->ZLD_DTLANC		:=  dData   
				// Mensal
				if lMensal
					ZLD->ZLD_DTCOLE	:= aRegs[i,nPosDataCol]
					ZLD->ZLD_FILENT		:= U_GetFilEnt(	aRegs[i,nPosRetiro],aRegs[i,nPosLoja],aRegs[i,nPosDataCol])
				else
					ZLD->ZLD_DTCOLE		:= 	dDtColeta                                                              
					ZLD->ZLD_FILENT		:= U_GetFilEnt(	aRegs[i,nPosRetiro],aRegs[i,nPosLoja],dDtColeta)
				endif
				ZLD->ZLD_SETOR 		:= 	cSetor
				ZLD->ZLD_LINROT		:= 	cLinRot
				ZLD->ZLD_TP_RL		:= 	cTipLR
				ZLD->ZLD_RETIRO		:= 	aRegs[i,nPosRetiro]
				ZLD->ZLD_RETILJ		:= 	aRegs[i,nPosLoja]
				ZLD->ZLD_DCRRET		:=  aRegs[i,nPosNomRet]
				ZLD->ZLD_FRETIS		:=	cFretist
				ZLD->ZLD_LJFRET		:= 	cLjFret
				ZLD->ZLD_VEICUL		:= 	cVeicul
				ZLD->ZLD_MOTOR		:= 	cMotor
				ZLD->ZLD_QTDBOM		:= 	aRegs[i,nPosQtdBom]
				ZLD->ZLD_KM			:= 	nTotKm
				ZLD->ZLD_STATUS		:= 	" "
				ZLD->ZLD_USER 		:= cUser
				ZLD->ZLD_TOTBOM		:= nTotBom   
				
				
				
				//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
				//³ Calcula vlr do frete                                 ³
				//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
				if cTpPagFret == "L"
					nVrFrete := nVrLitro *  aRegs[i,nPosQtdBom]
					ZLD->ZLD_VFRELT := nVrLitro
					ZLD->ZLD_VFREPR := nVrFrete
				elseif cTpPagFret == "K"
					nValTot := nTotkm * nValKM
					ZLD->ZLD_VFRELT := (nValTot * nProp) / aRegs[i,nPosQtdBom]
					ZLD->ZLD_VFREPR := (nValTot * nProp)
				elseif cTpPagFret == "F"
					ZLD->ZLD_VFRELT := (nValDia * nProp) / aRegs[i,nPosQtdBom]
					ZLD->ZLD_VFREPR := (nValDia * nProp)
				endif
				
				
				ZLD->(MsUnlock())
				ZLD->(dbCommit())
			endif
		endif
	next i
	
	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³ Salva numero do ticket                               ³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
	//putMv("LT_TICKET",strzero(val(cTicket)+1,6))
	
endif	

return

