static function vldMotor(cMotor)
local lRet := .T.
local aArea := getArea()

if !empty(cMotor)
	dbSelectArea("ZL0")
	dbSetOrder(1)
	dbgotop()
	lRet := dbSeek(xFilial("ZL0")+cMotor)
	
	if lRet .AND. !lPlataforma
		if ZL0->ZL0_FRETIS != cFretist
			xmaghelpfis("Atencao - Validacao","O Motorista selecionado nao pertence ao Transportador!","Selecione um motorista relacionado ao Transportador!")
			lRet:=.T.
		endif
		cDescMot := ZL0->ZL0_NOME
	endif
	
endif


restArea(aArea)
return lRet

