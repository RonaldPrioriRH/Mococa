static function delZLD(aRegs)
local aArea := getArea()
Local lContinue:=.f.

// -----------------------------------------------------------
// Se nao houver mais nenhum lancamento com esse ticket, entao 
// Deleta o Movimento Interno e continua exclusao
// -----------------------------------------------------------
if NoMoreTicket(cTicket,cSetor)
	if gerMI(cTicket,5)
		lContinue := .t.
	endif
else
	lContinue := .t.
endif

if lContinue                               
	dbSelectArea("ZLD")
	dbSetOrder(2) //ZLD_FILIAL+ZLD_CODREC+ZLD_RETIRO+ZLD_RETILJ
	for i := 1 to len(aRegs)
		if dbSeek(xFilial("ZLD")+cCodRec + aRegs[i,nPosRetiro] + aRegs[i,nPosLoja])
			recLock("ZLD",.F.)
			ZLD->(dbDelete())
			ZLD->(MSUnlock())
			ZLD->(dbCommit())
		endif
	next i
endif

restArea(aArea)
