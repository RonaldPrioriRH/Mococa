Static function isExist(cpVlr,cTab,cpCmpName)
local cRet := .F.
Local cQuery  := ""
Local aArea   := GetArea()
Local nT1 := 0
Local nT2 := 0
Local dLanc

cQuery := "SELECT * FROM "
cQuery += RetSqlName(cTab) + " WHERE D_E_L_E_T_ = ' ' "
cQuery += " AND "+cTab+"_FILIAL = '" + xFilial(cTab) + "'"
cQuery += " AND "+cpCmpName+" = '" + cpVlr + "'"
cQuery += " AND "+cTab+"_SETOR = '" + cSetor + "'"

if Select("TRB") > 0
	dbSelectArea("TRB")
	dbCloseArea("TRB")
endif

dbUseArea(.T.,"TOPCONN",TCGenQry(,,ALLTRIM(Upper(cQuery))),'TRB',.F.,.T.)

while TRB->(!Eof())
	nT1+=TRB->ZLD_QTDBOM
	nT2:=TRB->ZLD_TOTBOM
	dLanc:=stod(TRB->ZLD_DTCOLE)
	cRet:=.t.
	TRB->(dbskip())
enddo

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³ Zera variaveis - Caso mude o num. do ticket          ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
nVolAnt := 0
nLeiteBom := 0 
nTotBom:=0

If cRet 
	if MsgYesNo("O ticket digitado já existe! Deseja continuar?")
    	nVolAnt 	:= nT1
		nLeiteBom 	:= nT1
    	nTotBom 	:= nT2
		nLtDif 		:= nTotBom-nLeiteBom
		//dDtcoleta 	:= dLanc
	    cRet	:=.f.
	end
EndIf


dbSelectArea("TRB")
dbCloseArea("TRB")

RestArea(aArea)

Return cRet

