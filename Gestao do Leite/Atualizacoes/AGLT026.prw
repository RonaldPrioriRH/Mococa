Static Function getMovto(cpProd,cpLoja,cpDataIni,cpDataFim)

Local nRet:= 0
Local aArea := GetArea()

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ Conta registros de movimento do produtor                            ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
cQuery := "SELECT COUNT(*) AS QTDREG FROM " + RetSqlName("ZLD") + " ZLD "
cQuery += " WHERE D_E_L_E_T_ = ' '"
cQuery += " AND ZLD_FILIAL = '"+XFILIAL("ZLD")+"'  "
cQuery += " AND ZLD_RETIRO = '"+cpProd+"'  "
cQuery += " AND ZLD_RETILJ = '"+cpLoja+"'  "
cQuery += " AND ZLD_DTCOLE BETWEEN  '" + DTOS(cpDataIni) + "' AND '" + DTOS(cpDataFim) + "'"
If Select("TCT") <> 0
	TCT->(dbCloseArea("TCT"))
Endif
dbUseArea( .T., "TOPCONN", TcGenQry(,,cQuery), "TCT", .T., .F. )
if TCT->(!eof())
	nRet := TCT->QTDREG
endif
TCT->(dbclosearea())

restArea(aArea)

return nRet
