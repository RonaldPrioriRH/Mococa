Static Function runProdutores()

dbSelectArea("SA2")
dbSetOrder(1)
dbGoTop()

ProcRegua(RecCount())

While SA2->(!Eof())
	
	incproc()
	
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Se produtor tiver recp. de leite no periodo seta como Nao Bloqueado ณ
	//ณ Caso contrario como Bloqueado                                        ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	If SA2->A2_L_TPFOR $ "P/G/T"
		If getMovto(SA2->A2_COD,SA2->A2_LOJA,dt1,dt2) > 0
			setAtivo(SA2->A2_COD,SA2->A2_LOJA,"2")
		Else
			setAtivo(SA2->A2_COD,SA2->A2_LOJA,"1")
		EndIf
	EndIf
	
	SA2->(dbSkip())
EndDo
dbCloseArea()

Return

