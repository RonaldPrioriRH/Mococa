Static Function Grava()
Local cArea                         
Local bCampo   := { |nCPO| Field(nCPO) }

Private lMsHelpAuto := .T.
Private lMsErroAuto := .F.

If nOpcG # 2 // nao for consulta
    
	dbselectArea("ZLU")
	DbSetOrder(1)
	wProcura := dbseek(xFilial("ZLU")+M->ZLU_COD)
	If Inclui .or. Altera

		RecLock("ZLU",If(wProcura,.F.,.T.))
		ZLU->ZLU_FILIAL  := xFilial("ZLU")
		for x:=1 to len(aCpoEnchoice)
			wVar := "M->"+aCpoEnchoice[x]
			ax:="ZLU->"+aCpoEnchoice[x]
			if (POSICIONE("SX3",2,aCpoEnchoice[x],"X3_CONTEXT")!="V")
				&ax := &wVar
			endif
		next x
		MsUnlock()

		if Inclui .or. Altera
			ConfirmSx8()
		Endif

		
	Else  // exclusao

			RecLock("ZLU",.F.)
			dbdelete()
			MsUnlock()
			WriteSx2("ZLU")
		
	Endif
	
Endif

oDlg2:End()

Return(.T.)

