Static Function FXBOTAO()

Local nOpcA     := 0  //Avalia o botao utilizado
Local nUser     := 1  //Variavel para o List
Local oDlg            //Dialog Principal
Local oUsers          //Objeto List
Local cNome     := "" //Nome completo selecionado
Local cNReduz   := "" //Nome reduzido


PRIVATE aUsers    := {} // Array com todos os usuarios do sistema
PRIVATE aTmkUsers := {} // Array(ordenado por nome) com todos os usuarios do sistema mas somente os dados necessarios para a tela.
PRIVATE aTmkUSR1  := {} // Array com todos os usuarios do sistema mas somente os dados necessarios para a tela.
//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณRetorna array com os usuarios.     ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
aUsers:=AllUsers()

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณCarrega as informacoes necessarias.ณ
//ณ                                   ณ
//ณaTmkUsers[1] = Nome Completo       ณ
//ณaTmkUsers[2] = Cargo               ณ
//ณaTmkUsers[3] = Departamento        ณ
//ณaTmkUsers[4] = ID                  ณ 
//ณaTmkUsers[5] = Usuario             ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
For nI:=1 TO Len(aUsers)
	Aadd(aTmkUsr1,{aUsers[nI,1,4],aUsers[nI,1,13],aUsers[nI,1,12],aUsers[nI,1,1],aUsers[nI,1,2]})
Next nI

aTMKUsers := ASORT(aTmkUSR1,,, { |x, y| x[1] < y[1] }) //CRESCENTE Alfabetica

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณUsuarios                                                                ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู

DEFINE MSDIALOG oDlg TITLE "Usuarios" FROM 00,00 TO 250,717 PIXEL OF oMainWnd //Cadastro de Usuarios

	@02,02 TO 120,325 LABEL "" PIXEL OF oDlg
	@05,04 LISTBOX oUsers FIELDS ;
				HEADER ;  	
				"Nome Completo",;	// Nome Completo 
				"Cargo",;	// Cargo 
				"Depto",;   // Departamento
				"ID";       // Codigo do Usuario
				ON DbLCLICK (	nOpcA 	:= 1,;
								cNome	:= aTMKUsers[oUsers:nAt,1],;
								cNReduz	:= aTMKUsers[oUsers:nAt,5],;
								cCodUsu	:= aTMKUsers[oUsers:nAt,4],;
								oDlg:End()  );
				ON CHANGE (nUser:= oUsers:nAt) SIZE 320,115 OF oDlg PIXEL NOSCROLL
	oUsers:SetArray(aTMKUsers)
	oUsers:bLine:={||{ aTMKUsers[oUsers:nAt,1],;	// Nome 	
					   aTMKUsers[oUsers:nAt,2],;	// Cargo
					   aTMKUsers[oUsers:nAt,3],;	// Departamento
					   aTMKUsers[oUsers:nAt,4]}}	// Codigo do Usuario
						
	oUsers:Refresh()

	If (nOpc <> 2) .AND. (nOpc <> 5) 
		DEFINE SBUTTON FROM 05,328 TYPE 1 ENABLE OF oDlg ACTION (	nOpcA	:= 1,;
																	cNome	:= aTMKUsers[oUsers:nAt,1],;
																	cNReduz	:= aTMKUsers[oUsers:nAt,5],;
																	cCodUsu	:= aTMKUsers[oUsers:nAt,4],;
																	oDlg:End()  )
		DEFINE SBUTTON FROM 18,328 TYPE 2 ENABLE OF oDlg ACTION oDlg:End()
	Else
		DEFINE SBUTTON FROM 05,328 TYPE 2 ENABLE OF oDlg ACTION oDlg:End()	
	Endif

ACTIVATE MSDIALOG oDlg CENTERED

//Se o usuario confirmou Ok e a rotina for <> Visualizacao e <> Exclusao
If (nOpcA == 1)
	M->ZLU_USER  := cNReduz 
	M->ZLU_NOME  := cNome			// Atualiza os campos da tela
	M->ZLU_CODUSU:= cCodUsu
Endif

return