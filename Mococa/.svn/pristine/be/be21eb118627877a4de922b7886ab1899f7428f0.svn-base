METHOD ShowError(lInfo, lMostraErro) CLASS FatTriang
	Default lInfo := .T.
	Default lMostraErro := .F.
	if !empty(::cMsgErro)
		if ::lShowMsg
			if lInfo
				MsgInfo(::cMsgErro,"Atenção!")
			else
				MsgStop(::cMsgErro,"Atenção!")
			endif
			if lMostraErro
				MostraErro()
			endif
		else
			Conout(">> CLASS FatTriang: " + ::cMsgErro)
		endif
	endif
Return

