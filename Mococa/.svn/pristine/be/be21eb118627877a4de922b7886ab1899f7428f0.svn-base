METHOD CriaSX1(cPerg) CLASS FatTriang

	//Local nIndexId := 0

	if cPerg == "TBCFTRI1"

		U_xPutSx1(cPerg,"01","Carga ?"	  ,"","","mv_ch1","C",6,0,0,"G","U_TBCFTRI1('DAK')","DAK","","","mv_par01","","","","","","","","","","","","","","","","",{"Informe o numero da carga que tenha ","pedidos configurado para tirar estoque ","de outra filial."},{"",""},{"",""})
