		//nOpc=3 finaliza o processo atual e bloqueia o pedido
		ConOut("( WF APROVAÇÃO) Filial: "+ cxFilial+ "Rastreia o Pedido: "+cNum+"Processo: "+oProcess:fProcessID+'.'+oProcess:fTaskID )
