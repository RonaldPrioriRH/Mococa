					SC7->C7_OBS := AllTrim(SC7->C7_OBS)+"/"+cJusti
					Conout("( WF APROVAÇÃO) Data: "+dtoc(ddatabase)+ "Hora: "+time()+" Valor do SC7->C7_OBS: "+alltrim(SC7->C7_OBS)+" Valor do SC7->C7_CONAPRO: "+alltrim(SC7->C7_CONAPRO))
					SC7->(MsUnlock())
