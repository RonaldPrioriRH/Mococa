	nIni := nIni+6
	Next x*/


	dbSelectArea("SC7")
	SC7->(dbSetOrder(1))
	SC7->(dbgotop())

	if SC7->(dbSeek(cxFilial+cNum))

		//aCodAprov := FunCargEmail()
		//nQtdAprov := Len(aCodAprov)

		cGrupo := SC7->C7_APROV
		cPCLib := SC7->C7_NUM
