User Function AGLT043(__cArq)


//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ Declaracao de Variaveis                                             ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู

Local cLin:=""
Local nLi:=1
Private cPerg   := "AGLT043"
Private cString := "ZLZ"
Private dMes:= SubStr(Strtran(DTOC(Date()),"/",""),3,2)
Private dAno:= SubStr(Strtran(DTOC(Date()),"/",""),5,4)
Private dTime:= Strtran(Time(),":","")
Private cArqTxt:=AllTrim(__cArq)+"VL"+dMes+dAno+".txt" 
Private nHdl :=fCreate(cArqTxt)   
Private cEOL    := "CHR(13)+CHR(10)"

If Empty(cEOL)
	cEOL := CHR(13)+CHR(10)
Else
	cEOL := Trim(cEOL)
	cEOL := &cEOL
Endif

If nHdl == -1
	MsgAlert("O arquivo de nome "+cArqTxt+" nao pode ser executado! Verifique os parametros.","Atencao!")
	Return
Endif

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ Verifica se existe as perguntas no SX1 para crialas                 ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
CriaSx1()
Pergunte(cPerg,.T.)

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ Parametros para geracao:       ณ
//ณ mv_par01 -> Data de            ณ
//ณ mv_par02 -> Data at้           ณ
//ณ    ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู


cQuery:="SELECT DISTINCT ZLD_RETIRO, ZLD_RETILJ  FROM "+RetSqlName("ZLD")+ " ZLD WHERE ZLD_DTCOLE >= '"+dTos(MV_PAR01)+"' AND ZLD_DTCOLE <= '"+dTos(MV_PAR02)+"'"
cQuery+=" AND D_E_L_E_T_ = ' ' ORDER BY ZLD_RETIRO, ZLD_RETILJ"
TcQuery ChangeQuery(cQuery) New Alias "NAT"
NAT->(dbGoTop())



While NAT->(!EOF())
	
	//produtor; volume m้dio no perํodo solicitado-capacidade do tanque; dia impar/dia par; descri็ใo do produtor; c๓digo SIGSIF;
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Incrementa a regua                                                  ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู  
	
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Preenche campos para formacao da linha do CSV                       ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	 cLin+=LIMPCOD(Alltrim(NAT->ZLD_RETIRO))+";"     			   							  				//Codigo Produtor
	 cLin+=LIMPLOJA(Alltrim(NAT->ZLD_RETILJ))+";"					   						 				//Loja Produtor
	 cLin+=VolLeite(NAT->ZLD_RETIRO,NAT->ZLD_RETILJ,MV_PAR01,MV_PAR02)+"-"					   				//Volume 
	 _Tanque:=Posicione("SA2",1,xFilial("SA2")+NAT->ZLD_RETIRO+NAT->ZLD_RETILJ,"A2_L_TANQ")
	 IF Alltrim(_Tanque) <> '' 
	 	 nCapaci:=Posicione("ZLS",1,xFilial("ZLS")+_Tanque,"ZLS_CAPACI")
		 cLin+=Alltrim(Str(nCapaci))+";"                        			//Tanque
	 Else
	     cLin+="0;"
	 EndIF
	 cLin+=BuscaDia(NAT->ZLD_RETIRO,NAT->ZLD_RETILJ)+';'	   									   			//Dia impar dia par
	 cLin+=Alltrim(Posicione("SA2",1,xFilial("SA2")+NAT->ZLD_RETIRO+NAT->ZLD_RETILJ,"A2_NOME"))+";"		//Descricao produtor
	 cLin+=Alltrim(Posicione("SA2",1,xFilial("SA2")+NAT->ZLD_RETIRO+NAT->ZLD_RETILJ,"A2_L_SIGSI"))		//Codigo SIGSIF
	 cLin+=cEOL																				   				//Finalizacao de linha
	 
	If fWrite(nHdl,cLin,Len(cLin)) != Len(cLin)
		If !MsgAlert("Ocorreu um erro na gravacao do arquivo. Continua?","Atencao!")
			Exit
		Endif
	EndIf
	cLin:=""
	NAT->(dbSkip())
EndDo

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ O arquivo texto deve ser fechado, bem como o dialogo criado na fun- ณ
//ณ cao anterior.                                                       ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
NAT->(DbCloseArea("NAT"))
fClose(nHdl)
MsgInfo("Arquivo gerado com sucesso")
Return

