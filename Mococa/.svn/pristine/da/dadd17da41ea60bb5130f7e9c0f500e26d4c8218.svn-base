Static Function LIMPCOD(cCod)
Local nx              
Local cNCod:=''
Local lLoop:=.T.
Local lLetra:=.T.

For nx:=1 to len(cCod)
	If (SubStr(cCod,nx,1) = 'C' .OR. SubStr(cCod,nx,1)= 'c') .AND. lLetra
		lLetra:=.F.
		Loop
	Else
		If SubStr(cCod,nx,1) = '0'	.AND. lLoop
			Loop
		Else
			cNCod+=SubStr(cCod,nx,1)
			lLoop:=.F.
		EndIF
	EndIF
Next nX

