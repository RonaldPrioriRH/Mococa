Static Function ReportDef(cPerg)

	Private oReport,oSecZ0
	Private aTam := {}
	
	oReport := TReport():New("RGLT070","Relatório Prestações/Outros Débitos",cPerg,{|oReport|PrintReport(oReport)},"Este relatorio irá apresentar as prestações/outros debitos futuros.")
	
	oReport:lParamPage	:= .F.
	oReport:lHeaderVisible := .F.
	oReport:lFooterVisible := .F. 
	If _lTOTEM   
		oReport:cPrinterName	:= ALLTRIM(GETMV("MV_C_PRINT"))//"PDFCreator"//"HP LaserJet M1530 MFP Series PCL 6 - COOPRATA" //"Foxit Reader PDF Printer"
   		oReport:SetPreview(.F.)
		oReport:SetDevice(2)
   		oReport:SetEnvironment(2)  
		oReport:SetMsgPrint("Imprimindo...") 
	EndIf
	oReport:SetPortrait(.T.)
	oReport:nLineHeight	:= 45 
	oReport:Print(.F.,,.T.) 
	
Return oReport

