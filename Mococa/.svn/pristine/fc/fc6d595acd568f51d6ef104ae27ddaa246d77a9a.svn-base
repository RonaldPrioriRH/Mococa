Static Function FGLT0703(_cCodFil)

	Local _aArea    := GetArea()
	
	Local _cRet := " "
 
		
	//DbSelectArea("SM0")
	//DbSetOrder(1)
	SM0->(DbGoTop())
	
	While SM0->(!Eof())
	
		If SM0->M0_CODFIL = _cCodFil
			_cRet := SubString(SM0->M0_FILIAL,1,3)
			Exit
		EndIf
		SM0->(DbSkip())
	
	EndDo
	
	//DbCloseArea("SM0")
	
	RestArea(_aArea)
	
Return _cRet



