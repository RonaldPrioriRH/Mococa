User Function ExisteZM4(xcCod,xcLoja,xcOpc)  

Local _cAssoc := ""

If Select("TZM4") <> 0
	TZM4->(DbCloseArea())
EndIf

BeginSql alias "TZM4"
	SELECT * FROM %Table:ZM4% ZM4
		WHERE ZM4_FILIAL = %xFilial:ZM4%
		AND ZM4_CODSA2 = %Exp:xcCod%
		AND ZM4_LOJA = %Exp:xcLoja%
		AND ZM4.%NotDel%
EndSql

TZM4->(DbGoTop())

If !TZM4->(Eof())
	_cAssoc := TZM4->ZM4_CODIGO
EndIf

RETURN _cAssoc
