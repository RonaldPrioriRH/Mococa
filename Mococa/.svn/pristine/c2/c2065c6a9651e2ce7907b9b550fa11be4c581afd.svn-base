Private cArqCsv := ALLTRIM(MV_PAR03)
Private nHdl    := fCreate(cArqCsv)
Private cEOL    := "CHR(13)+CHR(10)"

If Empty(cEOL)
	cEOL := CHR(13)+CHR(10)
