User Function A020DELE()

Local cAlias    := Alias()
Local aAmb      := {}
Local lRet      := .T.
Local cProdutor := SA2->A2_COD
Local cLoja     := SA2->A2_LOJA

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณSalva o Ambiente.ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
aAmb := GetArea()

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ Salvando Integridade do Sistema. ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
dbSelectArea("SA1")
_nOrdSA1 := IndexOrd()
_nRecSA1 := Recno()

dbSelectArea("SA2")
_nOrdSA2 := IndexOrd()
_nRecSA2 := Recno()

dbSelectArea("SE2")
_nOrdSE2 := IndexOrd()
_nRecSE2 := Recno()

dbSelectArea("SE1")
_nOrdSE1 := IndexOrd()
_nRecSE1 := Recno()

dbSelectArea("SC5")
_nOrdSC5 := IndexOrd()
_nRecSC5 := Recno()

dbSelectArea("SF2")
_nOrdSF2 := IndexOrd()
_nRecSF2 := Recno()

dbSelectArea("SF1")
_nOrdSF1 := IndexOrd()
_nRecSF1 := Recno()

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ Validacao para verificar se existem movimentacoes para este Produtor  ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
dbSelectArea("SE1")
dbSetorder(2)//E1_FILIAL+E1_CLIENTE+E1_LOJA+E1_PREFIXO+E1_NUM+E1_PARCELA+E1_TIPO
dbSeek(xFILIAL("SE1")+cProdutor+cLoja,.T.)
If SE1->E1_FILIAL+SE1->E1_CLIENTE+SE1->E1_LOJA == xFILIAL("SE1")+cProdutor+cLoja
	lRet := .F.
	MsgAlert("Produtor nao pode ser excluido, existe Ctas a Receber para ele!")
EndIf

dbSelectArea("SC5")
dbSetorder(3)//C5_FILIAL+C5_CLIENTE+C5_LOJACLI+C5_NUM
dbSeek(xFILIAL("SC5")+cProdutor+cLoja,.T.)
If SC5->C5_FILIAL+SC5->C5_CLIENTE+SC5->C5_LOJACLI == xFILIAL("SC5")+cProdutor+cLoja
	lRet := .F.
	MsgAlert("Produtor nao pode ser excluido, existe Pedidos de Venda para ele!")
EndIf

dbSelectArea("SE2")
dbSetorder(6)//E2_FILIAL+E2_FORNECE+E2_LOJA+E2_PREFIXO+E2_NUM+E2_PARCELA+E2_TIPO
dbSeek(xFILIAL("SE2")+cProdutor+cLoja,.T.)
If SE2->E2_FILIAL+SE2->E2_FORNECE+SE2->E2_LOJA == xFILIAL("SE2")+cProdutor+cLoja
	lRet := .F.
	MsgAlert("Produtor nao pode ser excluido, existe Ctas a Pagar para ele!")
EndIf

dbSelectArea("SF2")
dbSetorder(2)//F2_FILIAL+F2_CLIENTE+F2_LOJA+F2_DOC+F2_SERIE
dbSeek(xFILIAL("SF2")+cProdutor+cLoja,.T.)
If SF2->F2_FILIAL+SF2->F2_CLIENTE+SF2->F2_LOJA == xFILIAL("SF2")+cProdutor+cLoja
	lRet := .F.
	MsgAlert("Produtor nao pode ser excluido, existe NF de Saida para ele!")
EndIf

dbSelectArea("SF1")
dbSetorder(2)//F1_FILIAL+F1_FORNECE+F1_LOJA+F1_DOC
dbSeek(xFILIAL("SF1")+cProdutor+cLoja,.T.)
If SF1->F1_FILIAL+SF1->F1_FORNECE+SF1->F1_LOJA == xFILIAL("SF1")+cProdutor+cLoja
	lRet := .F.
	MsgAlert("Produtor nao pode ser excluido, existe NF de Entrada para ele!")
EndIf

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ Verifica se existem movimentacoes nas rotinas do leite.   ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
lRet := U_MovProd(cProdutor,cLoja,.T.)

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ Posiciona no cadastro de cliente e deleta o Fornecedor.         ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
dbSelectArea("SA1")
dbSetorder(1)
dbSeek(xFILIAL("SA1")+cProdutor+cLoja)

If Found() .AND. lRet .AND. SA1->A1_L_TPCLI $ "PTE" //Produtor 
	RecLock("SA1",.F.)
	dbDelete()
	MsUnLock()
EndIf                  

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณRestaura a integridade. ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
dbSelectArea("SA1")
dbSetOrder(_nOrdSA1)
dbGoto(_nRecSA1)

dbSelectArea("SA2")
dbSetOrder(_nOrdSA2)
dbGoto(_nRecSA2)

dbSelectArea("SE2")
dbSetOrder(_nOrdSE2)
dbGoto(_nRecSE2)

dbSelectArea("SE1")
dbSetOrder(_nOrdSE1)
dbGoto(_nRecSE1)

dbSelectArea("SF2")
dbSetOrder(_nOrdSF2)
dbGoto(_nRecSF2)

dbSelectArea("SF1")
dbSetOrder(_nOrdSF1)
dbGoto(_nRecSF1)

dbSelectArea("SC5")
dbSetOrder(_nOrdSC5)
dbGoto(_nRecSC5)

dbSelectArea(cAlias)
//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณRestaura o Ambiene.ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
RestArea(aAmb)

Return(lRet)