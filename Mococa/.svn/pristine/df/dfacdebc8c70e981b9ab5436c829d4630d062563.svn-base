/*/
Static Function RunReport(Cabec1,Cabec2,Titulo,nLin)

Local nQtdReg := 0
Local nAux:=0
Local nQtdDif := 0
Local cAux := ""
Local dtAtual
Local x:=0
Local nSubTotal := 0
Local cUltLin := ""

Static dtini,dtfim
Static nTamCol := 7

Private aTotal := {}
Private aLinTotal:= {}

if len(alltrim(mv_par07)) != 2
	mv_par07 := strzero(val(mv_par07),2)
endif
if len(alltrim(mv_par06)) == 2
	mv_par06 := "20"+alltrim(mv_par06)
endif

dtini:=stod(mv_par06+mv_par07+"01")
dtfim:=lastday(dtini)


//--------------------------------
//  Monta Cabecalho com Dias
//--------------------------------
aLinTotal:={}
aTotal:={}
dtAtual := dtini
nAux:=0
while dtAtual <= dtfim
	nAux++
	aAdd(aTotal,0)
	aAdd(aLinTotal,0)
	Cabec1 += padr(strzero(nAux,2),nTamCol)
	dtAtual++
enddo

cQuery := "SELECT ZLD_LINROT,ZLD_RETIRO,ZLD_RETILJ,SUM(ZLD_QTDBOM) AS VOLUME "
cQuery += " FROM " + RetSqlName("ZLD") + " ZLD "
cQuery += " WHERE D_E_L_E_T_ = ' ' "
cQuery += " AND ZLD_SETOR = '" + mv_par01 + "'"
cQuery += " AND ZLD_DTCOLE BETWEEN '" + dtos(dtini) + "' AND '" + dtos(dtfim) + "' "
cQuery += " AND ZLD_RETIRO BETWEEN '" + mv_par02 + "' AND '" + mv_par04 + "' "
cQuery += " AND ZLD_RETILJ BETWEEN '" + mv_par03 + "' AND '" + mv_par05 + "' "
cQuery += " AND ZLD_LINROT BETWEEN '" + mv_par08 + "' AND '" + mv_par09 + "' "
cQuery += " AND ZLD_FILIAL = '" + xFilial("ZLD") + "'"
cQuery += " GROUP BY ZLD_LINROT,ZLD_RETIRO,ZLD_RETILJ "
cQuery += " ORDER BY ZLD_LINROT,ZLD_RETIRO,ZLD_RETILJ "


if Select("TRB") > 0
	dbSelectArea("TRB")
	dbCloseArea("TRB")
endif

dbUseArea(.T.,"TOPCONN",TCGenQry(,,ALLTRIM(Upper(cQuery))),'TRB',.F.,.T.)

COUNT TO nQtdReg

SetRegua(nQtdReg)

TRB->(dbgotop())
while TRB->(!Eof())
	
	Incregua()
	
	If lAbortPrint
		@nLin,00 PSAY "*** CANCELADO PELO OPERADOR ***"
		Exit
	Endif
	
	If nLin > 55 .or. cUltLin != TRB->ZLD_LINROT
		
		//--------------------------------
		// Mostra Total da Linha
		//--------------------------------
		if cUltLin != TRB->ZLD_LINROT
			if cUltLin != ""
				nLin := showSubTotal(nLin)
			endif
		endif
		
		//--------------------------------
		// Imprime Cabecalho
		//--------------------------------
		@00,00 psay AvalImp(220)
		nLin:=1
		@nLin,000 PSAY SM0->M0_NOMECOM+Space(16)+"CNPJ: "+Transform(SM0->M0_CGC,"@R X9.999.999/9999-99")
		nLin++
		@nLin,000 PSAY SubStr(SM0->M0_ENDENT,1,20)+" "+SubStr(SM0->M0_BAIRENT,1,17)+" "+SubStr(SM0->M0_CIDENT,1,13)+" "+SM0->M0_ESTENT+" Insc.E.: "+SM0->M0_INSC
		nLin++
		@nLin,000 PSAY __PrtThinLine()
		nLin++
		@nLin,100 PSAY "M A P A   A N A L I T I C O - "+mv_par07+"/"+mv_par06
		nLin++
		@nLin,000 PSAY __PrtThinLine()
		nLin++
		@nLin,000 PSAY Cabec1
		nLin++
		@nLin,000 PSAY __PrtThinLine()
		nLin++
		@nLin,000 PSAY TRB->ZLD_LINROT + "-" + POSICIONE("ZLW",1,XFILIAL("ZLW")+TRB->ZLD_LINROT,"ZLW_DESCRI")
		nLin++
	endif
	cUltLin := TRB->ZLD_LINROT
	
	//--------------------------------
	// Mostra PRODUTOR
	//--------------------------------
	@nLin,000 PSAY __PrtThinLine()
	nLin++
	if !empty(alltrim(TRB->ZLD_RETIRO))
		@nLin,000 PSAY TRB->ZLD_RETIRO+"-"+TRB->ZLD_RETILJ+" "+LEFT(POSICIONE("SA2",1,XFILIAL("SA2")+TRB->ZLD_RETIRO+TRB->ZLD_RETILJ,"A2_NOME"),30)
	endif
	nLin++
	
	//--------------------------------
	// Mostra valores dos dias do mes solicitado
	//--------------------------------
	nPos := 0
	x:=0
	dtAtual := dtini
	while dtAtual <= dtfim
		
		//--------------------------------
		// obtem volume do Produtor na data atual
		//--------------------------------
		//nAux :=  u_VolLeite(TRB->ZLD_FILIAL,dtAtual,dtAtual,TRB->ZLD_SETOR,TRB->ZLD_LINROT,TRB->ZLD_RETIRO,TRB->ZLD_RETILJ)
		nAux :=  gtVol(XFILIAL("ZLD"),mv_par01,TRB->ZLD_LINROT,TRB->ZLD_RETIRO,TRB->ZLD_RETILJ,dtAtual)
		@nLin,nPos PSAY transform(nAux,"@ 999999")
		
		//--------------------------------
		// armazena totais
		//--------------------------------
		x++
		aTotal[x] += nAux
		aLinTotal[x] += nAux
		nSubTotal += nAux
		
		//--------------------------------
		// proxima data
		//--------------------------------
		dtAtual++
		nPos += nTamCol
		
	enddo
	
	//--------------------------------
	// mostra total do fretista
	//--------------------------------
	@nLin,nPos PSAY transform(nSubTotal,"@ 9999999")
	nSubTotal := 0
	
	nLin++
	
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Impressao do cabecalho do relatorio. . .                            ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	/*
	If nLin > 55 // Salto de Pแgina. Neste caso o formulario tem 55 linhas...
	Cabec(Titulo,Cabec1,Cabec2,NomeProg,Tamanho,nTipo)
	nLin := 8
	Endif
	*/
	
	
	
	TRB->(dbSkip()) // Avanca o ponteiro do registro no arquivo
EndDo

TRB->(dbclosearea())


//--------------------------------
// Mostra Total da Linha
//--------------------------------
nLin := showSubTotal(nLin)
nLin++


//--------------------------------
// Mostra Total Geral
//--------------------------------
@nLin,000 PSAY __PrtThinLine()
nLin++

@nLin,000 PSAY "TOTAL GERAL"
nLin++

nPos:=0
x:=0
nAux:=0
dtAtual := dtini
while dtAtual <= dtfim
	x++
	@nLin,nPos PSAY transform(aTotal[x],"@ 9999999")
	
	nAux+=aTotal[x] // total geral
	
	nPos+=nTamCol
	dtAtual++
enddo
@nLin,nPos PSAY transform(nAux,"@ 9999999")


//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
