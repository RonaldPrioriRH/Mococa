Static Function gtVol(cpFilial,cpSetor,cpLinha,cpProd,cpLoja,dtcole)

Local nRet:= 0

cQuery := "SELECT SUM(ZLD_QTDBOM) AS TOTAL "
cQuery += " FROM " + RetSqlName("ZLD") + " ZLD "
cQuery += " WHERE D_E_L_E_T_ = ' ' "
cQuery += " AND ZLD_FILIAL = '" + cpFilial + "'"
cQuery += " AND ZLD_SETOR = '" + cpSetor + "'"
cQuery += " AND ZLD_LINROT = '" + cpLinha + "'"
cQuery += " AND ZLD_DTCOLE = '" + dtos(dtcole) + "' "
cQuery += " AND ZLD_RETIRO BETWEEN '" + cpProd + "' AND '" + cpProd + "' "
cQuery += " AND ZLD_RETILJ BETWEEN '" + cpLoja + "' AND '" + cpLoja + "' "

if Select("TRC") > 0
	dbSelectArea("TRC")
	dbCloseArea("TRC")
endif

dbUseArea(.T.,"TOPCONN",TCGenQry(,,ALLTRIM(Upper(cQuery))),'TRC',.F.,.T.)

if !TRC->(eof())
	nRet := TRC->TOTAL
endif
TRC->(dbclosearea())

return nRet