Static Function showSubTotal(npLin)

@npLin,000 PSAY __PrtThinLine()
npLin++
@npLin,000 PSAY "SUBTOTAL"
npLin++

nPos:=0
x:=0
nAux:=0
dtAtual := dtini
while dtAtual <= dtfim
	x++
	@npLin,nPos PSAY transform(aLinTotal[x],"@ 9999999")
	
	nAux+=aLinTotal[x] // total da linha
	aLinTotal[x]:=0
	
	nPos+=nTamCol
	dtAtual++
enddo
@npLin,nPos PSAY transform(nAux,"@ 9999999")

return npLin

